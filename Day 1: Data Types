#include <iostream>
#include <iomanip>
#include <limits>

using namespace std;

int main() {
    int i = 4;
    double d = 4.0;
    string s = "HackerRank ";
    int a;
    double b;
    string ss;
    scanf("%d",&a);
    scanf("%lf",&b);
    while (getline(cin,ss)){};
    std::cout << std::setprecision(1) << std::fixed;
    cout << a+i << endl;
    cout << b+d << endl;
    cout << s << ss        
    return 0;
}
    
